{
  "name": "limerence-pi-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "lint": "oxlint src scripts --config ./oxlintrc.json --tsconfig ./tsconfig.json --import-plugin --vitest-plugin --deny-warnings",
    "check:loc": "node scripts/check-loc.mjs",
    "static": "npm run typecheck && npm run lint && npm run check:loc",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@assistant-ui/react": "^0.12.10",
    "@assistant-ui/react-markdown": "^0.12.3",
    "@assistant-ui/react-syntax-highlighter": "^0.12.3",
    "@mariozechner/pi-ai": "^0.52.10",
    "@sinclair/typebox": "^0.34.0",
    "@supabase/supabase-js": "^2.95.3",
    "@tailwindcss/vite": "^4.1.17",
    "js-tiktoken": "^1.0.21",
    "lucide-react": "^0.544.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sql.js": "^1.14.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "oxlint": "^0.16.0",
    "typescript": "^5.7.3",
    "vite": "^7.1.6",
    "vitest": "^3.0.0"
  }
}
